{
  "name": "synctabs",
  "displayName": "Synctabs",
  "version": "0.0.1",
  "description": "sync tab between different browers",
  "author": "arronf2e@163.com",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "0.6.1",
    "@plasmohq/storage": "1.9.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.3",
    "@supabase/supabase-js": "2.38.4",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@radix-ui/colors": "^3.0.0",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33",
    "prettier": "3.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "5.2.2",
    "windy-radix-palette": "2.0.0-beta.7"
  },
  "manifest": {
    "key": "$CRX_KEY",
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "sidePanel",
      "scripting",
      "tabGroups",
      "tabs",
      "action"
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}
