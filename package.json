{
  "name": "synctabs",
  "displayName": "Synctabs",
  "version": "0.0.1",
  "description": "sync tab between different browers",
  "author": "arronf2e@163.com",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.38.4",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@plasmohq/messaging": "0.6.1",
    "@plasmohq/storage": "1.9.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "manifest": {
    "key": "$CRX_KEY",
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "sidePanel"
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}
